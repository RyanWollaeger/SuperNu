!-- group settings
in_wldex = 17
in_iswlread = t
in_ng = 5

!-- particle numbers
in_ns = 1000
in_npartmax = 100000

!-- imc tally method
in_isimcanlog = f
in_isddmcanlog = f
!-- ddmc mfp per cell
in_tauddmc = 5d0

!-- opacity switches
in_nobbopac = t
in_nobfopac = t
in_noffopac = t

&end

