&inputpars

!-- misc
in_nomp = 1
in_grab_stdout = f
in_puretran = f

!-- time step
in_tfirst = 2d0
in_tlast = 11d0
!in_nt = 1
in_nt = 192
in_alpha = 1d0

!-- gas grid settings
in_nr = 128
in_isvelocity = t
in_isshell = f
in_novolsrc = t
!!!!!!!!!!!!!!!!!!!!!!!!!!!!in_v0 = 2d8
in_velout = 0d0
!!!!!!!!!!!!!!!!!!!!!!!!!!!!in_l0 = 100d7
!!!!!!!!!!!!!!!!!!!!!!!!!!!!in_lr = 5d7
in_totmass = 0d0
